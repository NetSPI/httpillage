<div class="row">
  <div class="col-lg-12">
    <h1 class="page-title">Viewing <%= @user.email %></h1>
  </div>
</div>

<div class="row">
    <%= button_to "Edit User", edit_user_path(@user), 
      :class => "button-primary page-task-button",
      :method => :get
    %>
</div>

<h2>Jobs</h2>
<table class="jobs-table table table-striped">
  <tr>
    <th>ID</th>
    <th>Method</th>
    <th>URI</th>
    <th>Type</th>
    <th>Created</th>
    <th>Status</th>
  </tr>
<% @user_jobs.each do |j| %>
  <tr>
    <td><%= link_to j.id, show_job_path(j.id) %></td>
    <td><%= j.http_method %></td>
    <td><%= j.http_uri %></td>
    <td><%= j.attack_type %></td>
    <td><%= j.created_at %></td>
    <td><%= j.status %></td>
  </tr>
<% end %>
</table>