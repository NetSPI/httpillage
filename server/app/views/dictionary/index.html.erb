<div class="row">
	<div class="col-lg-12">
		<h1 class="page-title">Dictionaries</h1>
		<%= link_to '/dictionaries/new' do %>
			<button class="button-primary page-task-button">New Dictionary</button>
		<% end %>
	</div>
</div>

<table class="jobs-table table table-striped">
<tr>
	<th width="5">&nbsp;</th>
	<th>Filename</th>
	<th>Description</th>
</tr>
<% @dictionaries.all.each do |d| %>
<tr>
	<td><%= link_to '<i class="fa fa-paperclip"></i>'.html_safe, download_dictionary_path(d) %></td>
	<td><%= d.original_filename %></td>
	<td><%= d.description %></td>
</tr>
<% end %>