<div class="row">
	<div class="col-lg-12">
		<h1 class="page-title">Nodes</h1>
	</div>
</div>
<table class="jobs-table table table-striped">
	<tr>
		<th>ID</th>
		<th>IP Address</th>
		<th>MAC Address</th>
		<th>Last Seen</th>
		<th>Created</th>
	</tr>
	<% @nodes.each do |n| %>
	<tr>
		<td><%= n.id %></td>
		<td><%= n.ip_addr %></td>
		<td><%= n.mac_address %></td>
		<td><%= n.last_seen %></td>
		<td><%= n.created_at %></td>
	</tr>
	<% end %>
</table>